module HealthVault
  module WCData
    module Thing
      class Thing        
        @@class_hash = Hash.new  
        <% class_hash.each do |key, val| %>
        @@class_hash['<%= key %>'] = <%= val %><% end %>
        def self.guid_to_class(guid)
          return @@class_hash[guid]
        end        
      end
    end
  end
end
